FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            zeroGradient;
    }
    in
    {
        type            zeroGradient;
    }
    out
    {
        type            zeroGradient;
    }
    bottom_symW
    {
        type            zeroGradient;
    }
    bottom_pillar
    {
        type            zeroGradient;
    }
    channel
    {
        type            zeroGradient;
    }
}


